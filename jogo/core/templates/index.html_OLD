          </tr>
              <tr>
              <!-- this.id estÃ¡ passando o valor do id nesse caso quadro_1 -->
                <td onclick="velha(this.id);" id="quadro_1"></td> 
                <td onclick="velha(this.id);" id="quadro_2"></td>
